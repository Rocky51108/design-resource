# -*- coding: utf-8 -*-

height=float(input("身高="))
weight=float(input("体重="))
bim=weight/(height*height)
if bim<=18.5:
	print("你的BIM是%.2f你太瘦了"%(bim))
elif 18.5<bim<=25:
	print("你的BIM是%.2f你正常了"%(bim))
elif 25<bim<=28:
	print("你的BIM是%.2f你超重了"%(bim))
elif 28<bim<=32:
	print("你的BIM是%.2f你是个胖子"%(bim))
else:
	print("你的BIM是%.2f你就是一坨肉"%(bim))
